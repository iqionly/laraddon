{
    "name": "iqionly/laraddon",
    "description": "Laravel with support addons features",
    "type": "composer-plugin",
    "require": {
        "php": "^8.2",
        "composer-plugin-api": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Iqionly\\Laraddon\\": "src/",
            "Laraddon\\Attributes\\": "src/Attributes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iqionly\\Laraddon\\ExampleAddon\\": "examples/ExampleAddon/",
            "Iqionly\\Laraddon\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Iqionly"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Iqionly\\Laraddon\\LaraddonServiceProvider"
            ]
        },
        "class": "Iqionly\\Laraddon\\Plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "dev-main",
    "require-dev": {
        "illuminate/support": "^11.45",
        "illuminate/routing": "^11.45",
        "illuminate/container": "^11.45",
        "illuminate/database": "^11.45",
        "illuminate/view": "^11.45",
        "composer/composer": "^2.8",
        "phpstan/phpstan": "^2.1"
    }
}
